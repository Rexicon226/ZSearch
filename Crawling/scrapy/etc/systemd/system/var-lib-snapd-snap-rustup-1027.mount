[Unit]
Description=Mount unit for rustup, revision 1027
Before=snapd.service
After=zfs-mount.service

[Mount]
What=/var/lib/snapd/snaps/rustup_1027.snap
Where=/var/lib/snapd/snap/rustup/1027
Type=squashfs
Options=nodev,context=system_u:object_r:snappy_snap_t:s0,ro,x-gdu.hide,x-gvfs-hide
LazyUnmount=yes

[Install]
WantedBy=default.target multi-user.target
