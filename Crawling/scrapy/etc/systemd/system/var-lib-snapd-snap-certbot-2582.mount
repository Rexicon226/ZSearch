[Unit]
Description=Mount unit for certbot, revision 2582
Before=snapd.service
After=zfs-mount.service

[Mount]
What=/var/lib/snapd/snaps/certbot_2582.snap
Where=/var/lib/snapd/snap/certbot/2582
Type=squashfs
Options=nodev,context=system_u:object_r:snappy_snap_t:s0,ro,x-gdu.hide,x-gvfs-hide
LazyUnmount=yes

[Install]
WantedBy=multi-user.target
